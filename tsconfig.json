{
  
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "ES5",
    "lib": ["DOM", "ES2015"],
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
    
  }
}